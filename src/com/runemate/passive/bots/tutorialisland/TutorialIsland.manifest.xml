<manifest>
    <main-class>com.runemate.passive.bots.tutorialisland.TutorialIsland</main-class>
    <name>Passive Tutorial</name>
    <tag-line>Completes Tutorial Island</tag-line><!--Max of 50 chars-->
    <description>Just completes tutorial island. It's that simple!</description><!--Max of 110 chars-->
    <version>0.0.8</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>OTHER</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>TutorialIsland</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Tutorial</tag>
    </tags>
</manifest>